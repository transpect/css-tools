                  <xsl:variable name="state" select="p:lookahead1(10, $input, $state)"/> <!-- ':' -->
                  <xsl:variable name="state" select="p:consumeT(34, $input, $state)"/> <!-- ':' -->
                  <xsl:choose>
                    <xsl:when test="not($state[$p:error])">
