            <xsl:variable name="state" select="p:lookahead1(10, $input, $state)"/>  <!-- ':' -->
            <xsl:variable name="state" select="p:consume(34, $input, $state)"/>     <!-- ':' -->
            <xsl:sequence select="p:parse-declaration-1($input, $state)"/>
