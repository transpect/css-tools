                  </xsl:when>
                  <xsl:when test="$state[$p:lk] eq 1542">                           <!-- S IDENT -->
                    <xsl:variable name="state" select="p:lookahead3(22, $input, $state)"/> <!-- S | ':' -->
                    <xsl:sequence select="$state"/>
