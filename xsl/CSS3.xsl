              <xsl:variable name="state" as="item()+">
                <xsl:choose>
                  <xsl:when test="$state[$p:lk] eq 774">                            <!-- S S -->
                    <xsl:variable name="state" select="p:lookahead3(46, $input, $state)"/> <!-- S | IDENT | '"' | "'" | '/*' | '}' -->
