                  <xsl:variable name="state" select="p:strip-result($state)"/>
                  <xsl:variable name="state" select="p:consumeT(6, $input, $state)"/> <!-- S -->
                  <xsl:choose>
                    <xsl:when test="not($state[$p:error])">
