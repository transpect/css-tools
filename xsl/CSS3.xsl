            <xsl:choose>
              <xsl:when test="$state[$p:l1] eq 6">                                  <!-- S -->
                <xsl:variable name="state" select="p:lookahead2(68, $input, $state)"/> <!-- S | IDENT | FUNCTION | NUMBER | HEX | PERCENTAGE | PLUS |
